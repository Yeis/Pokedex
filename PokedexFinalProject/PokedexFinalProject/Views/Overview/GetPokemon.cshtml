@model  List<Pokemon> 
@{    
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>GetPokemon</title>
</head>
<body>
    
    <div>         
        @{ 
            foreach(Pokemon pok in Model)
            {
                <div class="col-sm-1">

                    <li>@Html.ActionLink(pok.Nombre, "GetDetails", "Overview", new { id = pok.PokemonID}, null)</li>
                </div>
            }
        }
    </div>
</body>
</html>
