@using Microsoft.AspNet.Identity
@using PokedexFinalProject



@if (SharedInstance.AppUser !=null)
{
    <li>@Html.ActionLink("Log Out", "Index", "Login", routeValues: null, htmlAttributes: new { id = "logOutLink" })</li>


    <ul class="nav navbar-nav navbar-right">
        
        <li>
            @Html.ActionLink("Hello " + SharedInstance.AppUser.Username + "!", "Index", "Overview", routeValues: null, htmlAttributes: new { title = "Manage" })
        </li>
       
    </ul>

   
}
else
{
    <ul class="nav navbar-nav navbar-right">
        <li>@Html.ActionLink("Register", "Register", "Login")</li>
        <li>@Html.ActionLink("Log in", "Index", "Login")</li>
        
    </ul>
}
